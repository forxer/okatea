<?xml version="1.0" encoding="utf-8"?>

<database>

	<action id="drop_mod_diary" label="Drop table %s" string="{{PREFIX}}mod_diary">
		<test eq="eq" value="{{PREFIX}}mod_diary" label="Table %s doesn't exists"
			type="wrn" string="{{PREFIX}}mod_diary">SHOW TABLES LIKE '{{PREFIX}}mod_diary'</test>
		DROP TABLE IF EXISTS `{{PREFIX}}mod_diary`
	</action>

</database>
