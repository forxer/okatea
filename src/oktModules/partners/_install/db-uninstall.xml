<?xml version="1.0" encoding="utf-8"?>

<database>

	<action id="drop_mod_partners" label="Drop table %s" string="{{PREFIX}}mod_partners">
		<test eq="eq" value="{{PREFIX}}mod_partners" label="Table %s doesn't exist" type="wrn"
		string="{{PREFIX}}mod_partners">SHOW TABLES LIKE '{{PREFIX}}mod_partners'</test>
		DROP TABLE IF EXISTS `{{PREFIX}}mod_partners`
	</action>

