		"symfony/config" : "~2.5",
		"symfony/debug" : "~2.5",
		"symfony/filesystem" : "~2.5",
		"symfony/finder" : "~2.5",
		"symfony/http-foundation" : "~2.5",
		"symfony/routing" : "~2.5",
		"symfony/templating" : "~2.5",
		"symfony/yaml" : "~2.5"
