{
	"name" : "okatea/okatea",
	"description" : "Another modular CMS in PHP",
	"type" : "project",
	"keywords" : [
		"cms",
		"modular",
		"php"
	],
	"homepage" : "http://okatea.org",
	"license" : "MIT",
	"authors" : [{
			"name" : "Vincent Garnier",
			"email" : "contact@okatea.org"
		}
	],
	"support" : {
		"email" : "contact@okatea.org",
		"issues" : "https://github.com/forxer/okatea/issues"
	},
	"minimum-stability" : "stable",
	"require" : {
		"php" : ">=5.4.0",
		"alchemy/zippy" : " ~0.1",
		"doctrine/dbal" : "~2.4",
		"dotclear/clearbricks" : "dev-default",
		"dunglas/php-socialshare" : "~0.1",
		"emberlabs/gravatarlib": "dev-master",
		"erusev/parsedown" : "~0.9",
		"ezyang/htmlpurifier" : "~4.6",
		"guzzle/guzzle" : "~3.7",
		"imagine/imagine" : "~0.5",
		"ircmaxell/password-compat" : "~1.0",
		"jdorn/sql-formatter" : "~1.2",
		"leafo/lessphp" : "~0.4",
		"maximebf/debugbar" : "~1.9",
		"mobiledetect/mobiledetectlib" : "~2.7",
		"monolog/monolog" : "~1.7",
		"nesbot/carbon" : "~1.8",
		"simplepie/simplepie" : "~1.3",
		"swiftmailer/swiftmailer" : "~5.0",
		"symfony/config" : "~2.4",
		"symfony/debug" : "~2.4",
		"symfony/filesystem" : "~2.4",
		"symfony/finder" : "~2.4",
		"symfony/http-foundation" : "~2.4",
		"symfony/routing" : "~2.4",
		"symfony/templating" : "~2.4",
		"symfony/yaml" : "~2.4"
	},
	"autoload" : {
		"psr-4" : {
			"Okatea\\" : "Okatea"
		},
		"classmap" : [
			"vendor/dotclear/clearbricks/common",
			"vendor/dotclear/clearbricks/filemanager",
			"vendor/dotclear/clearbricks/image",
			"vendor/dotclear/clearbricks/zip"
		],
		"files" : [
			"Okatea/functions.php"
		]
	}
}
